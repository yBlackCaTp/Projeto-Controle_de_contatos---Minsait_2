@{
    ViewData["Title"] = "Gerar Input Clientes";
}

<div class="text-center">

    <h1 class="display-4">Cadastro de Clientes</h1>

</div>

<form>
    <div class="mb-3">
        <label for="Cliente" class="form-label">Nome</label>
        <input type="text" class="form-control" placeholder="Digite o nome do cliente">
    </div>

    <div class="mb-3">
        <label for="E-mail" class="form-label">E-mail</label>
        <input type="email" class="form-control" placeholder="Digite o E-mail do cliente">
    </div>

    <div class="mb-3">
        <label for="telefone" class="form-label">Telefone</label>
        <input type="number" class="form-control" placeholder="Digite o telefone do cliente">
    </div>
   
    <button type="submit" class="btn btn-primary">Adicionar</button>
    <a class="btn btn-secondary" asp-controller="Contato" asp-action="Index">Voltar</a>
</form>

